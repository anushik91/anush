<script>
    function k(obj){
        var a1 = 1*obj.hu.value;
        var a2 = 1*obj.pe.value;
        var a3 = 1*obj.ma.value;
        var a4 = 1*obj.ap.value;
        var a5 = 1*obj.may.value;
        var a6 = 1*obj.hun.value;
        var a7 = 1*obj.hul.value;
        var a8 = 1*obj.og.value;
        var a9 = 1*obj.se.value;
        var a10 = 1*obj.ho.value;
        var a11 = 1*obj.no.value;
        var a12 = 1*obj.de.value;
        
        var s = (a1+a2+a3+a4+a5+a6+a7+a8+a9+a10+a11+a12)/12;
        obj.res.value = s;
    }
    
    function m(){
        obj.hu.value = null;
        obj.pe.value = "";
        obj.ma.value = "";
        obj.ap.value = "";
        obj.may.value = "";
        obj.hun.value = "";
        obj.hul.value = "";
        obj.og.value = "";
        obj.se.value = "";
        obj.ho.value = "";
        obj.no.value = "";
        obj.de.value = "";
    
    }
    
</script>


